{
  "name": "circle-cirlce",
  "description": "a reddit like website",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "./src/main.jsx",
  "author": "rickyxyz",
  "keywords": [
    "reactjs",
    "typescript",
    "vite",
    "vitest",
    "storybook",
    "lefthook",
    "eslint",
    "tailwind",
    "postcss",
    "prettier",
    "playwright"
  ],
  "scripts": {
    "firebase": "firebase emulators:start --import=./tests/assets/firebase-data",
    "firebase:seed": "firebase emulators:start --import=./tests/assets/firebase-data --export-on-exit",
    "firebase:kill": "kill-port 9099 8080 9199 4000",
    "dev": "concurrently \"firebase emulators:start --import=./tests/assets/firebase-data\" \"vite\"",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "firebase emulators:exec \"vitest run\"",
    "test:watch": "vitest watch",
    "test:e2e": "firebase emulators:exec --import=./tests/assets/firebase-data \"playwright test\"",
    "test:coverage": "vitest --coverage",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "playwright": "playwright test --ui"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "firebase": "^10.7.1",
    "localforage": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.49.2",
    "react-icons": "^4.12.0",
    "react-quill": "^2.0.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.18.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^3.0.1",
    "@hookform/resolvers": "^3.3.3",
    "@playwright/test": "^1.40.1",
    "@storybook/addon-essentials": "^7.6.3",
    "@storybook/addon-interactions": "^7.6.3",
    "@storybook/addon-links": "^7.6.3",
    "@storybook/addon-onboarding": "^1.0.9",
    "@storybook/blocks": "^7.6.3",
    "@storybook/builder-vite": "^7.6.4",
    "@storybook/react": "^7.6.3",
    "@storybook/react-vite": "^7.6.3",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
